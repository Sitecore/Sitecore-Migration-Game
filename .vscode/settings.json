        "activityBar.activeBackground": "#a2decb",
        "activityBar.background": "#a2decb",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#b072cd",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#a2decb",
        "statusBar.background": "#7dd0b6",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#58c2a1",
        "statusBarItem.remoteBackground": "#7dd0b6",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#7dd0b6",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#7dd0b699",
        "titleBar.inactiveForeground": "#15202b99"
